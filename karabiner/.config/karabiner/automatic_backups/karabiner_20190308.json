                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "grave_accent_and_tilde",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_control",
                                            "left_shift"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "osascript -e 'tell app \"iTerm2\"' -e 'create window with default profile' -e activate -e end"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
